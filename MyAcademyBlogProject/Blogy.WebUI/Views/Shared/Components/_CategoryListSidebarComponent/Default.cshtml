@model List<ResultCategoryDto>
@*
    Blog-Index CategoryList
*@
<div class="sidebar-box">
    <h3 class="heading">Kategoriler</h3>
    <ul class="categories">
        @foreach (var item in Model)
        {
            <li><a href="/Blog/GetBlogsByCategory/@item.Id">@item.CategoryName <span>(@item.Count)</span></a></li>
        }      
    </ul>
</div>
